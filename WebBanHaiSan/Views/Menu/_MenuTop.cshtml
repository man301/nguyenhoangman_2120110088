@model IEnumerable<WebBanHaiSan.Models.EF.Category>

@if (Model != null && Model.Any())
{
    <ul class="navbar_menu">
        @foreach (var item in Model)
        {
            if (item.Allias.Equals("trang-chu"))
            {
                item.Allias = "";
            }
            if (!string.IsNullOrEmpty(item.Link))
            {
                item.Allias = item.Link;
            }
            <li><a href="/@item.Allias">@item.Title</a></li>
        }

        @*<li><a href="#">shop</a></li>
            <li><a href="#">promotion</a></li>
            <li><a href="#">pages</a></li>
            <li><a href="#">blog</a></li>
            <li><a href="contact.html">contact</a></li>*@
    </ul>
}